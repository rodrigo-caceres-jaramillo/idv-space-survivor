config_version=5

[application]

config/name="SpaceSurvivor"
run/main_scene="res://src/ui/main_menu/main_menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Global="*res://src/main/singletons/Global.gd"
Events="*res://src/main/singletons/Events.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"

[debug]

gdscript/warnings/shadowed_global_identifier=0

[display]

window/size/viewport_width=800
window/size/viewport_height=450
window/size/window_width_override=1200
window/size/window_height_override=675
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
mouse_cursor/custom_image_hotspot=Vector2(8, 8)

[editor_plugins]

enabled=PackedStringArray("res://addons/phantom_camera/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "red",
"res://src/": "green",
"res://src/ui/": "blue"
}

